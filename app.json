{
  "expo": {
    "name": "job-app-frontend",
    "slug": "job-app-frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "jobappfrontend",

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true,
      "reactNativeNewArchitecture": true
    },

    "userInterfaceStyle": "automatic",

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.jobappfrontend"
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.jobappfrontend",
      "googleServicesFile": "./google-services.json"
    },

    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      "expo-font",
      [
        "expo-build-properties",
        {
          "android": {
            "newArchEnabled": true,
            "usesCleartextTraffic": true
          }
        }
      ]
    ],

    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2ca716d-a288-470c-aa9e-9d4a58f9c13a"
      }
    }
  }
}
